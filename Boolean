#include <iostream>
#include <stdio.h>
using namespace std;


int main(void)
{

    //Boolean 7
    printf("Boolean 7\n");

    int a,b,c;
    printf("a:");
    scanf ("%i", &a);

    printf("b:");
    scanf ("%i", &b);

    printf("c:");
    scanf ("%i", &c);

    printf("%d\n\n", ((a<b) & (b<c))|((a>b) & (b>c)));

    //Boolean 15
    printf("Boolean 15\n");

    int d,e,f;
    printf("d:");
    scanf ("%i", &d);

    printf("e:");
    scanf ("%i", &e);

    printf("f:");
    scanf ("%i", &f);

    printf( "%d\n\n", (d <= 0) & (e > 0) & (f > 0)) | ((d > 0 )& ( e <= 0) & (f > 0)) | ((d > 0)& (e > 0) & (f <= 0));


    //Boolean 26
    printf("Boolean 26\n");

    int x,y;
    printf("x:");
    scanf("%i", &x);

    printf("y:");
    scanf("%i", &y);

    printf("%d\n\n" , (x > 0) & ( y < 0));


    //Boolean 33
    printf(" Boolean 33\n");

    int a1,b1,c1;

    printf("a1:");
    scanf("%i", &a1);

    printf("b1:");
    scanf("%i", &b1);

    printf("c1:");
    scanf("%i", &c1);

    printf("%d\n\n", (a1 + b1 == c1) | (a1 + c1 == b1) | (b1 + c1 == a1));
    return 0;
}
